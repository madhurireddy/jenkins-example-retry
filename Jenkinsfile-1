pipeline {
  agent any
  stages {
    stage('stage 1') {
      steps {
        sh 'echo stage 1'
      }
    }
    stage('stage 2') {
      steps {
        script {
          def op = sh(returnStdout: true, script: 'docker build -t nginx .')
          println("Bla Bla : "+op)
        }
      }
    }
    stage('stage 3') {
      steps {
        sh 'echo stage 3'
      }
    }
  }
}
